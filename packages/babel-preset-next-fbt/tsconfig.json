{
  "extends": "@monteway/typescript/tsconfig-react-library.json",
  "compilerOptions": {
    "lib": ["DOM", "ESNext", "ES2021.String", "ESNext.AsyncIterable"],
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "ES2022",
    "target": "ES2020",
    "incremental": false,
    "jsx": "react-jsx"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
